<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:OOAdvantechApp.Resources;assembly=OOAdvantechApp"
             x:Class="OOAdvantechApp.MainPage">
  <StackLayout>
    <StackLayout>
      <Label Text="{local:ResoucesStrings Title}"
             VerticalOptions="Start"
             HorizontalTextAlignment="Center"
             IsVisible="true"
             FontSize="Large"
             FontAttributes="Bold"
             TextColor="Aqua" />
      <Button Text="Click"
               Command="{Binding ButtonClickCommand}">
      </Button>
      <Button Text="Load Storage Objects"
             Command="{Binding LoadObjectsCommand}">
      </Button>
      <Button Text="SQLite"
            Command="{Binding SQLiteCommand}">
      </Button>
     <Button Text="Barcode Scan"
            Command="{Binding BarcodeScanCommand}">
      </Button>
    </StackLayout>

  </StackLayout>
</ContentPage>